{
  "name": "personal-portfoli-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "NODE_OPTIONS=--openssl-legacy-provider; ng serve",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider && next dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider && next build && next export",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "publish": "set NODE_OPTIONS=--openssl-legacy-provider  && gulp build && ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.2.9",
